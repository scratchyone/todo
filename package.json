{
  "name": "to-do",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.scratchyone.com/todo/",
  "dependencies": {
    "gh-pages": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-snapshot": "^1.3.0",
    "tailwind": "^2.2.0",
    "tailwindcss": "^0.6.1"
  },
  "reactSnapshot": {
    "include": [
      "/todo",
      "/signin"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot && cp build/200.html build/404.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "tailwind": "tailwind build src/styles.css -o src/output.css"
  }
}
